#  pragma warning (pop)
#endif   // _RWSTD_MSVC


#if _RWSTD_DEFINE_TEMPLATE (_COMPLEX)
#  include <complex.cc>
#endif   // _RWSTD_DEFINE_TEMPLATE (_BASIC_OSTREAM)


#endif   // _RWSTD_COMPLEX_INCLUDED
