LC_CTYPE
copy "en_DK"
END LC_CTYPE

LC_COLLATE
copy "da_DK"
END LC_COLLATE

LC_MONETARY
copy "da_DK"
END LC_MONETARY

LC_NUMERIC
copy "da_DK"
END LC_NUMERIC

LC_TIME
abday    "<s><a><b>";"<a><t><a>";/
         "<m><a><r>";"<p><i><n>";/
         "<s><i><s>";"<t><a><l>";/
         "<a><r><f>"
