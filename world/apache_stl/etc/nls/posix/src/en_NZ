decimal_point           "<.>"
thousands_sep           "<,>"
grouping                3;3
END LC_NUMERIC

LC_TIME
abday       "<S><u><n>";"<M><o><n>";/
            "<T><u><e>";"<W><e><d>";/
            "<T><h><u>";"<F><r><i>";/
            "<S><a><t>"
day         "<S><u><n><d><a><y>";/
            "<M><o><n><d><a><y>";/
            "<T><u><e><s><d><a><y>";/
            "<W><e><d><n><e><s><d><a><y>";/
            "<T><h><u><r><s><d><a><y>";/
            "<F><r><i><d><a><y>";/
            "<S><a><t><u><r><d><a><y>"
abmon       "<J><a><n>";"<F><e><b>";/
            "<M><a><r>";"<A><p><r>";/
            "<M><a><y>";"<J><u><n>";/
            "<J><u><l>";"<A><u><g>";/
            "<S><e><p>";"<O><c><t>";/
            "<N><o><v>";"<D><e><c>"
mon         "<J><a><n><u><a><r><y>";/
            "<F><e><b><r><u><a><r><y>";/
            "<M><a><r><c><h>";/
            "<A><p><r><i><l>";/
            "<M><a><y>";/
            "<J><u><n><e>";/
            "<J><u><l><y>";/
            "<A><u><g><u><s><t>";/
            "<S><e><p><t><e><m><b><e><r>";/
            "<O><c><t><o><b><e><r>";/
            "<N><o><v><e><m><b><e><r>";/
            "<D><e><c><e><m><b><e><r>"
d_t_fmt     "<%><a><SP><%><d><SP><%><b><SP><%><Y><SP><%><T><SP><%><Z>"
d_fmt       "<%><d><//><%><m><//><%><y>"
t_fmt       "<%><T>"
am_pm       "";""
