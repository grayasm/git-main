decimal_point             "<,>"
thousands_sep             ""
grouping                  0;0
END LC_NUMERIC

LC_TIME
abday   "<D><o><m>";"<S><e><g>";/
        "<T><e><r>";"<Q><u><a>";/
        "<Q><u><i>";"<S><e><x>";/
        "<S><a'><b>"
day     "<D><o><m><i><n><g><o>";/
        "<S><e><g><u><n><d><a>";/
        "<T><e><r><c,><a>";/
        "<Q><u><a><r><t><a>";/
        "<Q><u><i><n><t><a>";/
        "<S><e><x><t><a>";/
        "<S><a'><b><a><d><o>"
abmon   "<J><a><n>";"<F><e><v>";/
        "<M><a><r>";"<A><b><r>";/
        "<M><a><i>";"<J><u><n>";/
        "<J><u><l>";"<A><g><o>";/
        "<S><e><t>";"<O><u><t>";/
        "<N><o><v>";"<D><e><z>"
mon     "<J><a><n><e><i><r><o>";/
        "<F><e><v><e><r><e><i><r><o>";/
        "<M><a><r><c,><o>";/
        "<A><b><r><i><l>";/
        "<M><a><i><o>";/
        "<J><u><n><h><o>";/
        "<J><u><l><h><o>";/
        "<A><g><o><s><t><o>";/
        "<S><e><t><e><m><b><r><o>";/
        "<O><u><t><u><b><r><o>";/
        "<N><o><v><e><m><b><r><o>";/
        "<D><e><z><e><m><b><r><o>"
d_t_fmt "<%><a><SP><%><d><SP><%><b><SP><%><Y><SP><%><T><SP><%><Z>"
d_fmt   "<%><d><-><%><m><-><%><Y>"
t_fmt   "<%><T>"
am_pm   "";""
