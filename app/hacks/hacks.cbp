<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="hacks" />
		<Option makefile_is_custom="1" />
		<Option execution_dir="../hacks/src" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="all">
				<Option output="src/hacks" prefix_auto="1" extension_auto="1" />
				<Option type="0" />
				<Option compiler="gcc" />
				<MakeCommands>
					<Build command="mingw32-make.exe -f $makefile $target" />
					<CompileFile command="mingw32-make.exe -f $makefile $file" />
					<Clean command="mingw32-make.exe -f $makefile clean" />
					<DistClean command="$make -f $makefile distclean$target" />
					<AskRebuildNeeded command="mingw32-make.exe -q -f $makefile $target" />
					<SilentBuild command="mingw32-make.exe -f $makefile $target &gt; $(CMD_NULL)" />
				</MakeCommands>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wall" />
			<Add option="-fexceptions" />
		</Compiler>
		<Unit filename="Makefile">
			<Option target="&lt;{~None~}&gt;" />
		</Unit>
		<Unit filename="src/Makefile">
			<Option target="&lt;{~None~}&gt;" />
		</Unit>
		<Unit filename="src/main.cpp">
			<Option target="&lt;{~None~}&gt;" />
		</Unit>
		<Extensions>
			<DoxyBlocks>
				<comment_style block="0" line="0" />
				<doxyfile_project />
				<doxyfile_build />
				<doxyfile_warnings />
				<doxyfile_output />
				<doxyfile_dot />
				<general />
			</DoxyBlocks>
			<code_completion />
			<envvars />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
