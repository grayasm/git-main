            <F N="../src/test_vector.hpp"/>
        </Folder>
        <Folder
            Name="Resource Files"
            Filters="*.ico;*.cur;*.dlg"/>
        <Folder
            Name="Bitmaps"
            Filters="*.bmp"/>
        <Folder
            Name="Other Files"
            Filters="">
            <F
                N="../src/Makefile"
                Type="Makefile"/>
            <F
                N="../src/Makefile.am"
                Type="Makefile"/>
        </Folder>
    </Files>
</Project>
